{
    "ConnectionStrings": {
        "AzureStorageConnection": "UseDevelopmentStorage=true"
    },
    "AzureStorage": {
        "Tables": {
            "CustomersTable": "Customers",
            "ProductsTable": "Products"
        },
        "Blobs": {
            "ImagesContainer": "product-images"
        },
        "Queues": {
            "OrderEventsQueue": "order-events"
        },
        "Files": {
            "ContractsShare": "contracts"
        },
        "SasExpiryMinutes": 60
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}